			<StackPanel.ContextFlyout>
				<MenuFlyout>
					<ToggleMenuFlyoutItem
						x:Name="tmfi_status_bar_draw_2" x:Uid="tmfi_status_bar_draw" Click="status_bar_click" Text="Draw Tool" />
					<ToggleMenuFlyoutItem
						x:Name="tmfi_status_bar_unit_2" x:Uid="tmfi_status_bar_unit" Click="status_bar_click" Text="Len Unit Name" />
					<ToggleMenuFlyoutItem
						x:Name="tmfi_status_bar_pos_2" x:Uid="tmfi_status_bar_pos" Click="status_bar_click" Text="Cursor Position" />
					<ToggleMenuFlyoutItem
						x:Name="tmfi_status_bar_grid_2" x:Uid="tmfi_status_bar_grid" Click="status_bar_click" Text="Grid Size" />
					<ToggleMenuFlyoutItem
						x:Name="tmfi_status_bar_page_2" x:Uid="tmfi_status_bar_page" Click="status_bar_click" Text="Page Size" />
					<ToggleMenuFlyoutItem
						x:Name="tmfi_status_bar_zoom_2" x:Uid="tmfi_status_bar_zoom" Click="status_bar_click" Text="Scale" />
				</MenuFlyout>
			</StackPanel.ContextFlyout>
			<!-- ContextFlyout を処理するため event_released を設定 -->
			<StackPanel
