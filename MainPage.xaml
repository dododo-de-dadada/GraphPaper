		<!-- メインパネル -->
		<RelativePanel x:Name="main_relative_panel" Grid.Row="1" Grid.Column="0" >
			<SwapChainPanel
				x:Name="scp_main_panel"
				CompositionScaleChanged="main_panel_scale_changed"
				KeyboardAcceleratorPlacementMode="Hidden"
				Loaded="main_panel_loaded"
				PointerCanceled="event_canceled"
				PointerEntered="event_entered" PointerExited="event_exited"
				PointerMoved="event_moved" PointerPressed="event_pressed"
				PointerReleased="event_released" PointerWheelChanged="event_wheel_changed"
				RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignTopWithPanel="True"
				RelativePanel.AlignRightWithPanel="True" RelativePanel.AlignBottomWithPanel="True"
				SizeChanged="main_panel_size_changed">
				<!--
				通常は, MenuFlyoutItem でキーボードアクセラレータを指定すべきだが,
				そうすると SwapChainPanel をクリックしたあとアクセラレーターが機能しなくなる.
				フォーカスのせいかもしれないが, 原因は不明.
				そこでこのエレメントにアクセラレーターを設定する.
				通常は, ScopeOwner をすぐ上の親に限定すべきだが, 
				そうすると, 親より上のオブジェクトは既存ではなく, 新規に作成
				されてしまう。つまり, メンバ変数にアクセスできない。
				-->
				<SwapChainPanel.KeyboardAccelerators>
					<KeyboardAccelerator Invoked="kacc_escape_invoked" Key="Escape"/>
					<KeyboardAccelerator Invoked="kacc_back_invoked" Key="Back"/>
					<KeyboardAccelerator Invoked="kacc_delete_invoked" Key="Delete"/>
					<KeyboardAccelerator Invoked="kacc_delete_shift_invoked" Key="Delete" Modifiers="Shift"/>
					<KeyboardAccelerator Invoked="kacc_down_invoked" Key="Down"/>
					<KeyboardAccelerator Invoked="kacc_down_shift_invoked" Key="Down" Modifiers="Shift"/>
					<KeyboardAccelerator Invoked="kacc_enter_invoked" Key="Enter"/>
					<KeyboardAccelerator Invoked="kacc_left_invoked" Key="Left"/>
					<KeyboardAccelerator Invoked="kacc_left_shift_invoked" Key="Left" Modifiers="Shift"/>
					<KeyboardAccelerator Invoked="kacc_right_invoked" Key="Right"/>
					<KeyboardAccelerator Invoked="kacc_right_shift_invoked" Key="Right" Modifiers="Shift"/>
					<KeyboardAccelerator Invoked="kacc_up_invoked" Key="Up"/>
					<KeyboardAccelerator Invoked="kacc_up_shift_invoked" Key="Up" Modifiers="Shift"/>
					<!--
					<KeyboardAccelerator Invoked="copy_invoked" Key="C" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="paste_invoked" Key="V" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="delete_invoked" Key="Delete" />
					<KeyboardAccelerator Invoked="undo_invoked" Key="Z" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="redo_invoked" Key="Y" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="cut_invoked" Key="X" Modifiers="Control" />
					<KeyboardAccelerator Invoked="select_all_invoked" Key="A" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="bring_to_front_invoked" Key="End" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="bring_forward_invoked" Key="PageDown" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="send_backward_invoked" Key="PageUp" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="send_to_back_invoked" Key="Home" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="group_invoked" Key="G" Modifiers="Control" />
					<KeyboardAccelerator Invoked="ungroup_invoked" Key="U" Modifiers="Control"/>
					<KeyboardAccelerator Invoked="find_text_invoked" Key="F" Modifiers="Control"/>
					-->
				</SwapChainPanel.KeyboardAccelerators>
			</SwapChainPanel>
