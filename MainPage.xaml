			</StackPanel>
		</ContentDialog>
		<!-- 貼り付け確認ダイアログ -->
		<!--
		<ContentDialog x:Name="cd_conf_paste_dialog" x:Uid="cd_conf_paste_dialog" CloseButtonText="Cancel" DefaultButton="Close" PrimaryButtonText="Paste">
			<StackPanel Orientation="Horizontal" VerticalAlignment="Center" >
				<FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="{StaticResource glyph_conf}" />
				<TextBlock x:Uid="tk_conf_paste" />
			</StackPanel>
		</ContentDialog>
		-->
		<!-- 保存確認ダイアログ -->
		<ContentDialog x:Name="cd_conf_save_dialog" x:Uid="cd_conf_save_dialog" CloseButtonText="Cancel" DefaultButton="Close" PrimaryButtonText="Save" SecondaryButtonText="Don't Save">
			<StackPanel Orientation="Horizontal" VerticalAlignment="Center" >
				<FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="{StaticResource glyph_conf}" Margin="0,0,16,0" />
				<TextBlock x:Uid="tk_conf_save"/>
			</StackPanel>
		</ContentDialog>
		<!-- メッセージダイアログ -->
		<ContentDialog x:Name="cd_message_dialog" x:Uid="cd_message_dialog" CloseButtonText="Close" DefaultButton="Close">
			<StackPanel Orientation="Horizontal" VerticalAlignment="Top" >
				<FontIcon x:Name="fi_message" FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="{StaticResource glyph_block}" Margin="0,0,16,0"/>
				<TextBlock x:Name="tk_message"/>
			</StackPanel>
		</ContentDialog>
		<!-- 用紙の大きさダイアログ -->
		<ContentDialog
			x:Name="cd_sheet_size_dialog" x:Uid="cd_sheet_size_dialog" CloseButtonText="Close"
			DefaultButton="Close" PrimaryButtonText="Apply" SecondaryButtonText="Adjust to Shapes">
			<Grid>
				<Grid.RowDefinitions>
					<RowDefinition />
					<RowDefinition />
					<RowDefinition />
					<RowDefinition />
					<RowDefinition />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition />
					<ColumnDefinition />
					<ColumnDefinition />
					<ColumnDefinition />
				</Grid.ColumnDefinitions>
				<Grid.Resources>
					<Style TargetType="muxc:NumberBox">
						<Setter Property="PlaceholderText" Value="NaN"/>
						<Setter Property="SpinButtonPlacementMode" Value="Hidden"/>
						<Setter Property="HorizontalAlignment" Value="Stretch" />
						<Setter Property="VerticalAlignment" Value="Top" />
						<Setter Property="Grid.ColumnSpan" Value="2" />
						<Setter Property="SmallChange" Value="1" />
						<Setter Property="LargeChange" Value="10" />
					</Style>
				</Grid.Resources>
				<muxc:NumberBox
					x:Name="tx_sheet_size_width" x:Uid="tx_sheet_size_width" Grid.Column="0" Grid.Row="0"
					Header="Width" Margin="0,0,4,12" ToolTipService.ToolTip="Must be between 1-32768" ValueChanged="sheet_size_value_changed" />
				<muxc:NumberBox
					x:Name="tx_sheet_size_height" x:Uid="tx_sheet_size_height" Grid.Column="2" Grid.Row="0"
					Header="Height" Margin="4,0,0,12" ToolTipService.ToolTip="Must be between 1-32768" ValueChanged="sheet_size_value_changed" />
				<muxc:NumberBox
					x:Name="tx_sheet_padding_top" x:Uid="tx_sheet_padding_top" Grid.Column="1" Grid.Row="1"
					Header="Top Padding" Margin="0,0,0,12" ToolTipService.ToolTip="Must be less than sheet height" ValueChanged="sheet_size_value_changed" />
				<muxc:NumberBox
					x:Name="tx_sheet_padding_left" x:Uid="tx_sheet_padding_left" Grid.Column="0" Grid.Row="2"
					Header="Left Padding" Margin="0,0,4,12" ToolTipService.ToolTip="Must be less than sheet width" ValueChanged="sheet_size_value_changed" />
				<muxc:NumberBox
					x:Name="tx_sheet_padding_right" x:Uid="tx_sheet_padding_right" Grid.Column="2" Grid.Row="2"
					Header="Right Padding" Margin="4,0,0,12" ToolTipService.ToolTip="Must be less than sheet width" ValueChanged="sheet_size_value_changed" />
				<muxc:NumberBox
					x:Name="tx_sheet_padding_bottom" x:Uid="tx_sheet_padding_bottom" Grid.Column="1" Grid.Row="3"
					Header="Bottom Padding" Margin="0,0,0,12" ToolTipService.ToolTip="Must be less than sheet height" ValueChanged="sheet_size_value_changed" />
				<ComboBox
					x:Name="cb_len_unit" x:Uid="cb_len_unit" Header="Length Unit"
					Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="4" Margin="0,0,0,12"
					SelectionChanged="sheet_size_selection_changed">
					<ComboBoxItem x:Name="cbi_len_unit_pixel" x:Uid="cbi_len_unit_pixel" Content="Pixel" IsSelected="True"/>
					<ComboBoxItem x:Name="cbi_len_unit_inch" x:Uid="cbi_len_unit_inch" Content="Inch"/>
					<ComboBoxItem x:Name="cbi_len_unit_milli" x:Uid="cbi_len_unit_milli" Content="Millimeter"/>
					<ComboBoxItem x:Name="cbi_len_unit_point" x:Uid="cbi_len_unit_point" Content="Point"/>
					<ComboBoxItem x:Name="cbi_len_unit_grid" x:Uid="cbi_len_unit_grid" Content="Grid" />
				</ComboBox>
			</Grid>
		</ContentDialog>
		<!-- 頂点をくっつける閾値 -->
		<ContentDialog x:Name="cd_snap_point" x:Uid="cd_snap_point" CloseButtonText="Close" DefaultButton="Close" PrimaryButtonText="Apply" >
			<StackPanel>
				<Slider x:Name="sd_snap_point" x:Uid="sd_snap_point" Maximum="127.5" Minimum="0" SnapsTo="Ticks" TickFrequency="0.5" Header="Thresholds" />
			</StackPanel>
		</ContentDialog>
		<!-- 属性ダイアログ -->
		<ContentDialog
			x:Name="cd_dialog_prop" CloseButtonText="Close" Closed="dialog_prop_closed" 
			DefaultButton="Close" Opened="dialog_prop_opened" PrimaryButtonText="Apply" 
			Unloaded="dialog_prop_unloaded">
			<RelativePanel>
				<SwapChainPanel
					x:Name="scp_dialog_panel"
					BorderBrush="{ThemeResource ButtonBackgroundThemeBrush}" BorderThickness="1"
					CornerRadius="1" Padding="1"
					CompositionScaleChanged="dialog_panel_scale_changed" Height="170"
					Loaded="dialog_panel_loaded" Margin="0,0,0,12" MaxHeight="170" MaxWidth="170"
					RelativePanel.AlignTopWithPanel="True" 
					RelativePanel.AlignHorizontalCenterWithPanel="True"
					SizeChanged="dialog_panel_size_changed" Width="170"
					/>
				<!-- Height を指定するこれ大事 -->
				<ListView x:Name="lv_dialog_list" Loaded="dialog_list_loaded" RelativePanel.Below="scp_dialog_panel" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" RelativePanel.AlignBottomWithPanel="True" Visibility="Collapsed" Width="256" />
				<Slider
					x:Name="dialog_slider_0" x:Uid = "dialog_slider_0"
					IsThumbToolTipEnabled="False" Margin="0,0,0,4" Maximum="255" Minimum="0"
					RelativePanel.Below="scp_dialog_panel" RelativePanel.AlignLeftWithPanel="True"
					RelativePanel.AlignRightWithPanel="True" StepFrequency="1"
					Visibility="Collapsed" />
				<Slider
					x:Name="dialog_slider_1" x:Uid = "dialog_slider_1"
					IsThumbToolTipEnabled="False" Margin="0,0,0,4" Maximum="255" Minimum="0"
					RelativePanel.Below="dialog_slider_0" RelativePanel.AlignLeftWithPanel="True"
					RelativePanel.AlignRightWithPanel="True" StepFrequency="1"
					Visibility="Collapsed" />
				<Slider
					x:Name="dialog_slider_2" x:Uid = "dialog_slider_2"
					IsThumbToolTipEnabled="False" Margin="0,0,0,4" Maximum="255" Minimum="0"
					RelativePanel.Below="dialog_slider_1" RelativePanel.AlignLeftWithPanel="True"
					RelativePanel.AlignRightWithPanel="True" StepFrequency="1"
					Visibility="Collapsed" />
				<Slider
					x:Name="dialog_slider_3" x:Uid = "dialog_slider_3"
					IsThumbToolTipEnabled="False" Margin="0,0,0,4" Maximum="255" Minimum="0"
					RelativePanel.Below="dialog_slider_2" RelativePanel.AlignLeftWithPanel="True"
					RelativePanel.AlignRightWithPanel="True" StepFrequency="1"
					Visibility="Collapsed" />
				<Slider
					x:Name="dialog_slider_4" x:Uid = "dialog_slider_4"
					IsThumbToolTipEnabled="False" Margin="0,0,0,4" Maximum="255" Minimum="0" 
					RelativePanel.Below="dialog_slider_3" RelativePanel.AlignLeftWithPanel="True"
					RelativePanel.AlignRightWithPanel="True" StepFrequency="1"
					Visibility="Collapsed" />
				<TextBlock x:Name="tb_version" x:Uid="tb_version" RelativePanel.Below="dialog_slider_4" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" Visibility="Collapsed" />
				<CheckBox
					x:Name="dialog_check_box" x:Uid="dialog_check_box" Content="Check Box"
					IsChecked="False" Margin="0,0,0,4" RelativePanel.Below="dialog_slider_4"
					RelativePanel.AlignLeftWithPanel="True" Visibility="Collapsed" />
				<StackPanel x:Name="sp_combo_box" Margin="0,0,0,4" Orientation="Horizontal" RelativePanel.AlignLeftWithPanel="True" RelativePanel.Below="dialog_check_box">
					<ComboBox x:Name="dialog_combo_box_0" x:Uid="dialog_combo_box_0" Header="?" Margin="0,0,12,0" Visibility="Collapsed" />
					<ComboBox x:Name="dialog_combo_box_1" x:Uid="dialog_combo_box_1" Header="?" Visibility="Collapsed" />
				</StackPanel>
				<muxc:RadioButtons
					x:Name="dialog_radio_btns" MaxColumns="2" Margin="0,0,0,4"
					RelativePanel.AlignLeftWithPanel="True" RelativePanel.Below="sp_combo_box" Visibility="Collapsed" >
					<!--
					x:String などの UIElement でない要素のリストでは,
					オブザベーションのタイミングかなんかでうまくチェックがつかなかったりする.
					そこで UIElement であるラディオボタンを要素にする. 重複して使用する例については
					https://learn.microsoft.com/ja-jp/windows/apps/design/controls/radio-button を参照
					-->
					<RadioButton x:Name="dialog_radio_btn_0"/>
					<RadioButton x:Name="dialog_radio_btn_1"/>
				</muxc:RadioButtons>
			</RelativePanel>
		</ContentDialog>
		<!-- 印刷のための透明なキャンバス -->
		<!--<Canvas x:Name="PrintCanvas" Opacity="0" />-->
	</Grid>
</Page>
