<Page
    x:Class="GraphPaper.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GraphPaper"
    xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Page.Resources>
        <ResourceDictionary>
            <!-- 曲線アイコンの描画コマンド -->
            <x:String x:Key="data_bezi">
                M0.560633,8.093141 C7.686753,1.656054 7.654862,15.655831 16.030224,8.749841
                L15.310918,7.967942 C8.311178,14.40621 8.311178,0.405251 -0.032292,7.249824 Z
            </x:String>
            <!-- 破線アイコンの描画コマンド -->
            <x:String x:Key="data_dash">
                M0, 8 L3, 8 L3, 9 L0, 9 Z
                M6, 8 L10, 8 L10, 9 L6, 9 Z
                M13, 8 L16, 8 L16, 9 L13, 9 Z
            </x:String>
            <!-- 点線アイコンの描画コマンド -->
            <x:String x:Key="data_dot">
                M0, 8 L0.5, 8 L0.5, 9 L0, 9 Z
                M3.5, 8 L4.5, 8 L4.5, 9 L3.5, 9 Z
                M7.5, 8 L8.5, 8 L8.5, 9 L7.5, 9 Z
                M11.5, 8 L12.5, 8 L12.5, 9 L11.5, 9 Z
                M15.5, 8 L16, 8 L16, 9 L15.5, 9 Z
            </x:String>
            <!-- 一点破線アイコンの描画コマンド -->
            <x:String x:Key="data_dash_dot">
                M0.5, 8 L4.5, 8 L4.5, 9 L0.5, 9 Z
                M7.5, 8 L8.5, 8 L8.5, 9 L7.5, 9 Z
                M11.5, 8 L15.5, 8 L15.5, 9 L11.5, 9 Z
            </x:String>
            <!-- 二点破線アイコンの描画コマンド -->
            <x:String x:Key="data_dash_dot_dot">
                M0, 8 L2.5, 8 L2.5, 9 L0, 9 Z
                M5.5, 8 L6.5, 8 L6.5, 9 L5.5, 9 Z
                M9.5, 8 L10.5, 8 L10.5, 9 L9.5, 9 Z
                M13.5, 8 L16, 8 L16, 9 L13.5, 9 Z
            </x:String>
            <!-- だ円アイコンの描画コマンド -->
            <x:String x:Key="data_elli">
                M8,2 A8,6 0 0 1 8,14 A8,6 0 0 1 8,2 Z
                M8,3 A7,5 0 0 1 8,13 A7,5 0 0 1 8,3 Z
            </x:String>
            <!-- クループアイコンの描画コマンド -->
            <x:String x:Key="data_group">
                M0,0 L3,0 L3,1 L11,1 L11,11 L1,11 L1,3 L0,3 Z
                M3,3 L3,2 L10,2 L10,10 L2,10 L2,3 Z
                M11,5 L15,5 L15,13 L16,13 L16,16 L13,16 L13,15 L5,15 L5,11 L6,11 L6,14 L13,14 L13,13 L14,13 L14,6 L11,6 Z
                M13,0 L16,0 L16,3 L13,3 Z
                M0,13 L3,13 L3,16 L0,16 Z
            </x:String>
            <!-- 直線アイコンの描画コマンド -->
            <x:String x:Key="data_line">
                M0,8 L16,8 L16,9 L0,9 Z
            </x:String>
            <!-- ひし型アイコンの描画コマンド -->
            <x:String x:Key="data_quad">
                M8,2 L16,8 L8,14 L0,8 Z
                M8,3.33333 L14.66666,8 L8,12.66666 L1.33333,8 Z
            </x:String>
            <!-- 方形アイコンの描画コマンド -->
            <x:String x:Key="data_rect">
                M0,2 L16,2 L16,14 L0,14 Z
                M1,3 L15,3 L15,13 L1,13 Z
            </x:String>
            <!-- 角丸方形アイコンの描画コマンド -->
            <x:String x:Key="data_rrect">
                M5,2 L11,2 A5,5 0 0 1 16,7 L16,9 A5,5, 0 0 1 11,14 L5,14 A5,5 0 0 1 0,9 L0,7 A5,5 0 0 1 5,2 Z
                M5,3 L11,3 A4,4 0 0 1 15,7 L15,9 A4,4, 0 0 1 11,13 L5,13 A4,4 0 0 1 1,9 L1,7 A4,4 0 0 1 5,3 Z
            </x:String>
            <!-- 目盛りアイコンの描画コマンド -->
            <x:String x:Key="data_scale">
                M3,1 L4,1 L4,2 L8,2 L8,3 L4,3 L4,5 L6,5 L6,6 L4,6 L4,8 L8,8 L8,9 L4,9 L4,11 L6,11 L6,12 L4,12 L4,14 L13,14 L13,1 L14,1 L14,15 L3,15 Z
            </x:String>
            <!-- 図形選択アイコンの描画コマンド -->
            <x:String x:Key="data_select">
                M0,0 H1 V1 H2 V2 H3 V3 H4 V4 H5 V5 H6 V6 H7 V7 H8 V8 H9 V9 H10 V10 H11 V11 H7 V12 H8 V14 H9 V17 H6 V16 H5 V14 H4 V12 H3 V13 H2 V14 H1 V15 H0 Z
                M1,2 H2 V3 H3 V4 H4 V5 H5 V6 H6 V7 H7 V8 H8 V9 H9 V10 H6 V12 H7 V14 H8 V16 H6 V14 H5 V12 H4 V11 H3 V12 H2 V13 H1 Z
            </x:String>
            <!-- 文字列アイコンの描画コマンド -->
            <x:String x:Key="data_text">
                M1.25 16.00 L7.25 1.00 L8.75 1.00 L14.75 16.00 L13.25 16.00 L11.75 12.25 L4.25 12.25 L2.75 16.00 Z
                M4.75 11.00 L8.00 2.875 L11.25 11.00 Z
            </x:String>
            <!-- グループ解除アイコンの描画コマンド -->
            <x:String x:Key="data_ungroup">
                M1,1 L11,1 L11,11 L1,11 Z
                M2,2 L10,2 L10,10 L2,10 Z
                M11,5 L15,5 L15,15 L5,15 L5,11 L6,11 L6,14 L14,14 L14,6 L11,6 Z
            </x:String>
            <!-- 開いた矢印アイコンの描画コマンド -->
            <x:String x:Key="data_arrow_opened">
                M0, 8 L9.3, 8 L2.75, 6.52 L3, 5.48 L16, 8.5 L3, 11.52 L2.75, 10.48 L9.3, 9 L0, 9 Z
            </x:String>
            <!-- 閉じた矢印アイコンの描画コマンド -->
            <x:String x:Key="data_arrow_filled">
                M0, 8 L3, 8 L3, 5.48 L16, 8.5 L3, 11.52 L3,9 L0, 9 Z
            </x:String>
            <PathIcon x:Key="icon_dash" Data="{StaticResource data_dash}" Width="16" Height="16" />
            <!-- 均等アイコンの描画コマンド -->
            <!--
            <x:String x:Key="data_align_justified">
                M0,1 L16,1 L16,2 L0,2 Z
                M0,4 L4,4 L4,5 L0,5 Z M6,4 L10,4 L10,5 L6,5 Z M12,4 L16,4 L16,5 L12,5 Z
                M0,7 L16,7 L16,8 L0,8 Z
                M0,10 L4,10 L4,11 L0,11 Z M6,10 L10,10 L10,11 L6,11 Z M12,10 L16,10 L16,11 L12,11 Z
                M0,13 L16,13 L16,14 L0,14 Z
            </x:String>
            -->
            <!-- 上よせアイコンの描画コマンド -->
            <!--
            <x:String x:Key="data_align_top">
                M0,0 L12,0 L12,1 L0,1 Z M0,3 L16,3 L16,4 L0,4 Z M0,6 L12,6 L12,7 L0,7 Z
                M0,14 L2,14 L2,15 L0,15 Z M4,14 L6,14 L6,15 L4,15 Z M8,14 L10,14 L10,15 L8,15 Z M12,14 L14,14 L14,15 L12,15 Z
            </x:String>
            -->
            <!-- 下よせアイコンの描画コマンド -->
            <!--
            <x:String x:Key="data_align_bot">
                M0,0 L2,0 L2,1 L0,1 Z M4, 0 L6, 0 L6, 1 L4, 1 Z M8, 0 L10, 0 L10, 1 L8, 1 Z M12, 0 L14, 0 L14, 1 L12, 1 Z
                M0,8 L12,8 L12,9 L0,9 Z M0,11 L16,11 L16,12 L0,12 Z M0,14 L12,14 L12,15 L0,15 Z
            </x:String>
            -->
            <!-- 中段アイコンの描画コマンド -->
            <!--
            <x:String x:Key="data_align_mid">
                M0,0 L2,0 L2,1 L0,1 Z M4, 0 L6, 0 L6, 1 L4, 1 Z M8, 0 L10, 0 L10, 1 L8, 1 Z M12, 0 L14, 0 L14, 1 L12, 1 Z
                M0,4 L12,4 L12,5 L0,5 Z M0,7 L16,7 L16,8 L0,8 Z M0,10 L12,10 L12,11 L0,11 Z
                M0,14 L2,14 L2,15 L0,15 Z M4,14 L6,14 L6,15 L4,15 Z M8,14 L10,14 L10,15 L8,15 Z M12,14 L14,14 L14,15 L12,15 Z
            </x:String>
            -->
            <!-- アプリケーションデータを格納するファイル名 -->
            <x:String x:Key="file_app_data">
                app_data
            </x:String>
            <!-- ストロークコンテキストメニュー -->
            <MenuFlyout x:Key="mf_stroke">
                <MenuFlyoutSubItem x:Uid="mfsi_stroke_style" Text="Style">
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_stroke_solid_2" x:Uid="rmfi_stroke_solid" Click="rmfi_stroke_solid_click" Text="Solid" GroupName="gn_stroke_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_line}" VerticalAlignment="Center" Width="16" Height="16" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <MenuFlyoutSeparator />
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_stroke_dash_2" x:Uid="rmfi_stroke_dash" Click="rmfi_stroke_dash_click" Text="Dash" GroupName="gn_stroke_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_dash}" VerticalAlignment="Center" Width="16" Height="16" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_stroke_dot_2" x:Uid="rmfi_stroke_dot" Click="rmfi_stroke_dot_click" Text="Dot" GroupName="gn_stroke_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_dot}" VerticalAlignment="Center" Width="16" Height="16" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_stroke_dash_dot_2" x:Uid="rmfi_stroke_dash_dot" Click="rmfi_stroke_dash_dot_click" Text="Dash Dot" GroupName="gn_stroke_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_dash_dot}" VerticalAlignment="Center" Width="16" Height="16" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_stroke_dash_dot_dot_2" x:Uid="rmfi_stroke_dash_dot_dot" Click="rmfi_stroke_dash_dot_dot_click" Text="Dash Dot Dot" GroupName="gn_stroke_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_dash_dot_dot}" VerticalAlignment="Center" Width="16" Height="16" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                </MenuFlyoutSubItem>
                <MenuFlyoutItem x:Name="mfi_stroke_pattern_2" x:Uid="mfi_stroke_pattern" Click="mfi_stroke_pattern_click" IsEnabled="False" Text="Dash Pattern..." />
                <MenuFlyoutSeparator />
                <MenuFlyoutItem x:Uid="mfi_stroke_width" Click="mfi_stroke_width_click" Text="Width..." />
                <MenuFlyoutSeparator />
                <MenuFlyoutSubItem x:Uid="mfsi_arrow_style" Text="Arrowhead Style">
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_arrow_none_2" x:Uid="rmfi_arrow_none" Click="rmfi_arrow_none_click" Text="None" GroupName="gn_arrow_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_line}" VerticalAlignment="Center" Width="16" Height="16" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <MenuFlyoutSeparator />
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_arrow_opened_2" x:Uid="rmfi_arrow_opened" Click="rmfi_arrow_opened_click" Text="Opened" GroupName="gn_arrow_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_arrow_opened}" VerticalAlignment="Center" Width="16" Height="16" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_arrow_filled_2" x:Uid="rmfi_arrow_filled" Click="rmfi_arrow_filled_click" Text="Filled" GroupName="gn_arrow_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_arrow_filled}" VerticalAlignment="Center" Width="16" Height="16" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                </MenuFlyoutSubItem>
                <MenuFlyoutItem x:Name="mfi_arrow_size_2" x:Uid="mfi_arrow_size" Click="mfi_arrow_size_click" IsEnabled="False" Text="Arrowhead Size..." />
                <MenuFlyoutSeparator />
                <MenuFlyoutItem x:Uid="mfi_stroke_color" Click="mfi_stroke_color_click" Text="Color..." />
            </MenuFlyout>
            <!-- 塗りつぶしコンテキストメニュー -->
            <MenuFlyout x:Key="mf_fill">
                <MenuFlyoutItem x:Uid="mfi_fill_color" Click="mfi_fill_color_click" Text="Color..." />
            </MenuFlyout>
            <!-- 書体コンテキストメニュー -->
            <MenuFlyout x:Key="mf_font">
                <MenuFlyoutItem x:Uid="mfi_font_family" Click="mfi_font_family_click" Icon="Font" Text="Family..." />
                <MenuFlyoutItem x:Uid="mfi_font_size" Click="mfi_font_size_click" Icon="FontSize" Text="Size..." />
                <MenuFlyoutItem x:Uid="mfi_font_weight" Click="mfi_font_weight_click" Icon="Bold" Text="Weight..." />
                <MenuFlyoutItem x:Uid="mfi_font_stretch" Click="mfi_font_stretch_click" Text="Stretch..."/>
                <MenuFlyoutSubItem x:Uid="mfsi_font_style" Icon="Italic" Text="Style">
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_font_normal_2" x:Uid="rmfi_font_normal" Click="rmfi_font_normal_click" Text="Normal" GroupName="gn_font_style"/>
                    <MenuFlyoutSeparator />
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_font_oblique_2" x:Uid="rmfi_font_oblique" Click="rmfi_font_oblique_click" Text="Normal" GroupName="gn_font_style"/>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_font_italic_2" x:Uid="rmfi_font_italic" Click="rmfi_font_italic_click" Text="Normal" GroupName="gn_font_style"/>
                </MenuFlyoutSubItem>
                <MenuFlyoutSeparator />
                <MenuFlyoutSubItem x:Uid="mfsi_text_align_t" Text="Text Alignment">
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_text_align_left_2" x:Uid="rmfi_text_align_left" Click="rmfi_text_align_left_click" Text="Left" GroupName="gn_text_align_t"/>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_text_align_right_2" x:Uid="rmfi_text_align_right" Click="rmfi_text_align_right_click" Text="Right" GroupName="gn_text_align_t"/>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_text_align_center_2" x:Uid="rmfi_text_align_center" Click="rmfi_text_align_center_click" Text="Center" GroupName="gn_text_align_t" />
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_text_align_justified_2" x:Uid="rmfi_text_align_justified" Click="rmfi_text_align_justified_click" Text="Justified" GroupName="gn_text_align_t" />
                </MenuFlyoutSubItem>
                <MenuFlyoutSubItem x:Uid="mfsi_text_align_p" Text="Paragraph Alignment">
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_text_align_top_2" x:Uid="rmfi_text_align_top" Click="rmfi_text_align_top_click" Text="Top" GroupName="gn_text_align_p" />
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_text_align_bottom_2" x:Uid="rmfi_text_align_bottom" Click="rmfi_text_align_bottom_click" Text="Bottom" GroupName="gn_text_align_p" />
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_text_align_middle_2" x:Uid="rmfi_text_align_middle" Click="rmfi_text_align_middle_click" Text="Middle" GroupName="gn_text_align_p" />
                </MenuFlyoutSubItem>
                <MenuFlyoutSubItem x:Uid="mfsi_text_line" Text="Line Spacing">
                    <MenuFlyoutItem x:Uid="mfi_text_line_contract" Click="mfi_text_line_contract_click" Text="Condense" />
                    <MenuFlyoutItem x:Uid="mfi_text_line_expand" Click="mfi_text_line_expand_click" Text="Expand" />
                    <MenuFlyoutSeparator />
                    <MenuFlyoutItem x:Uid="mfi_text_line" Click="mfi_text_line_click" Text="Height" />
                </MenuFlyoutSubItem>
                <MenuFlyoutItem x:Uid="mfi_text_margin" Click="mfi_text_margin_click" Text="Margin..." />
                <MenuFlyoutSeparator />
                <MenuFlyoutItem x:Uid="mfi_font_color" Click="mfi_font_color_click" Text="Color..." />
            </MenuFlyout>
            <!-- ページコンテキストメニュー -->
            <MenuFlyout x:Key="mf_page">
                <MenuFlyoutSubItem x:Uid="mfsi_grid_show" Text="Show Grid">
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_grid_show_hide_2" x:Uid="rmfi_grid_show_hide" Click="rmfi_grid_show_hide_click" Text="Hide" GroupName="gn_grid_show"/>
                    <MenuFlyoutSeparator />
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_grid_show_back_2" x:Uid="rmfi_grid_show_back" Click="rmfi_grid_show_back_click" Text="Back" GroupName="gn_grid_show"/>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_grid_show_front_2" x:Uid="rmfi_grid_show_front" Click="rmfi_grid_show_front_click" Text="Front" GroupName="gn_grid_show"/>
                </MenuFlyoutSubItem>
                <ToggleMenuFlyoutItem x:Name="tmfi_grid_snap_2" x:Uid="tmfi_grid_snap" Click="tmfi_grid_snap_click"  Text="Snap to Grid" />
                <MenuFlyoutSubItem x:Uid="mfsi_grid_size" Text="Size Grid">
                    <MenuFlyoutItem x:Uid="mfi_grid_contract" Click="mfi_grid_len_contract_click" Text="Condense" />
                    <MenuFlyoutItem x:Uid="mfi_grid_expand" Click="mfi_grid_len_expand_click" Text="Expand" />
                    <MenuFlyoutSeparator />
                    <MenuFlyoutItem x:Uid="mfi_grid_len" Click="mfi_grid_len_click" Text="Length..." />
                </MenuFlyoutSubItem>
                <MenuFlyoutItem x:Uid="mfi_grid_opac" Click="mfi_grid_opac_click" Text="Opacity..." />
                <MenuFlyoutSeparator />
                <MenuFlyoutSubItem x:Uid="mfsi_zoom" Text="Zoom">
                    <MenuFlyoutItem x:Uid="mfi_zoom_in" Click="mfi_zoom_in_clicked" Icon="ZoomIn" Text="In" />
                    <MenuFlyoutItem x:Uid="mfi_zoom_out" Click="mfi_zoom_out_clicked" Icon="ZoomOut" Text="Out" />
                    <MenuFlyoutSeparator />
                    <MenuFlyoutItem x:Uid="mfi_zoom_reset" Click="mfi_zoom_reset_click" KeyboardAcceleratorTextOverride="Ctrl+0" Icon="Zoom" Text="Reset" />
                </MenuFlyoutSubItem>
                <MenuFlyoutItem x:Uid="mfi_page_size" Click="mfi_page_size_click" Text="Size..." />
                <MenuFlyoutItem x:Uid="mfi_page_color" Click="mfi_page_color_click" Text="Color..." />
                <MenuFlyoutItem x:Uid="mfi_page_unit" Click="mfi_page_unit_click" Text="Unit..." />
            </MenuFlyout>
            <!-- グループコンテキストメニュー -->
            <MenuFlyout x:Key="mf_ungroup">
                <MenuFlyoutItem x:Name="mfi_ungroup_2" x:Uid="mfi_ungroup" Click="mfi_ungroup_click" KeyboardAcceleratorTextOverride="Ctrl+U" Text="Ungroup" >
                    <MenuFlyoutItem.Icon>
                        <PathIcon Data="{StaticResource data_ungroup}" VerticalAlignment="Center" Width="16" Height="16"  />
                    </MenuFlyoutItem.Icon>
                </MenuFlyoutItem>
            </MenuFlyout>
        </ResourceDictionary>
    </Page.Resources>
    <Grid Margin="0,0,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>
        <!-- メインメニュー -->
        <muxc:MenuBar Grid.Row="0" Grid.ColumnSpan="2" Background="{ThemeResource SystemBaseLowColor}">
            <!-- ファイルメニュー -->
            <muxc:MenuBarItem x:Uid="mbi_file" AccessKey="F" Title="File">
                <MenuFlyoutItem x:Name="mfi_new" x:Uid="mfi_new" Click="mfi_new_click" Icon="Page2" Text="New" >
                    <MenuFlyoutItem.KeyboardAccelerators>
                        <KeyboardAccelerator Key="N" Modifiers="Control" />
                    </MenuFlyoutItem.KeyboardAccelerators>
                </MenuFlyoutItem>
                <MenuFlyoutItem x:Name="mfi_open" x:Uid="mfi_open" Click="mfi_open_click" Icon="OpenFile" Text="Open...">
                    <MenuFlyoutItem.KeyboardAccelerators>
                        <KeyboardAccelerator Key="O" Modifiers="Control" />
                    </MenuFlyoutItem.KeyboardAccelerators>
                </MenuFlyoutItem>
                <MenuFlyoutItem x:Name="mfi_save" x:Uid="mfi_save" Click="mfi_save_click" Icon="Save" Text="Save" >
                    <MenuFlyoutItem.KeyboardAccelerators>
                        <KeyboardAccelerator Key="S" Modifiers="Control" />
                    </MenuFlyoutItem.KeyboardAccelerators>
                </MenuFlyoutItem>
                <MenuFlyoutItem x:Name="mfi_save_as" x:Uid="mfi_save_as" Click="mfi_save_as_click" Text="Save As..." >
                    <MenuFlyoutItem.Icon>
                        <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="&#xE792;" />
                    </MenuFlyoutItem.Icon>
                    <MenuFlyoutItem.KeyboardAccelerators>
                        <KeyboardAccelerator Key="S" Modifiers="Control,Shift" />
                    </MenuFlyoutItem.KeyboardAccelerators>
                </MenuFlyoutItem>
                <MenuFlyoutSeparator />
                <MenuFlyoutSubItem x:Uid="mfsi_recent_files" Text="Recent Files">
                    <MenuFlyoutSubItem.Icon>
                        <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="&#xE823;" />
                    </MenuFlyoutSubItem.Icon>
                    <MenuFlyoutItem x:Name="mfi_recent_files_1" Click="mfi_recent_files_1_click" Text="1" />
                    <MenuFlyoutItem x:Name="mfi_recent_files_2" Click="mfi_recent_files_2_click" Text="2" />
                    <MenuFlyoutItem x:Name="mfi_recent_files_3" Click="mfi_recent_files_3_click" Text="3" />
                    <MenuFlyoutItem x:Name="mfi_recent_files_4" Click="mfi_recent_files_4_click" Text="4" />
                    <MenuFlyoutItem x:Name="mfi_recent_files_5" Click="mfi_recent_files_5_click" Text="5" />
                </MenuFlyoutSubItem>
                <MenuFlyoutSeparator />
