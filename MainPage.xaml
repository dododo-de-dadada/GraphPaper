					</PathIcon.RenderTransform>
				</PathIcon>
				<Rectangle x:Name="r_eyedropper" Width="16" Height="16" Stroke="Black" Fill="Black" Visibility="Collapsed" />
			</StackPanel>
			<StackPanel x:Name="sp_status_bar_unit" Margin="0,0,12,0" Orientation="Horizontal">
				<TextBlock x:Uid="tk_status_bar_unit" Text="Unit:"/>
				<TextBlock x:Name="tk_status_bar_unit" Text="0"/>
			</StackPanel>
			<StackPanel x:Name="sp_status_bar_pos" Margin="0,0,12,0" Orientation="Horizontal">
				<TextBlock x:Uid="tk_status_bar_pos_x" Text="X:" />
				<TextBlock x:Name="tk_status_bar_pos_x" Margin="0,0,4,0" Text="0"/>
				<TextBlock x:Uid="tk_status_bar_pos_y" Text="Y:" />
				<TextBlock x:Name="tk_status_bar_pos_y" Text="0"/>
			</StackPanel>
			<StackPanel x:Name="sp_status_bar_grid" Margin="0,0,12,0" Orientation="Horizontal">
				<TextBlock x:Uid="tk_status_bar_grid" Text="Grid:"/>
				<TextBlock x:Name="tk_status_bar_grid" Text="0" />
			</StackPanel>
			<StackPanel x:Name="sp_status_bar_page" Margin="0,0,12,0" Orientation="Horizontal">
				<TextBlock x:Uid="tk_status_bar_page_w" Text="W:"/>
				<TextBlock x:Name="tk_status_bar_page_w" Margin="0,0,4,0" Text="0"/>
				<TextBlock x:Uid="tk_status_bar_page_h" Text="H:"/>
				<TextBlock x:Name="tk_status_bar_page_h" Text="0"/>
			</StackPanel>
			<StackPanel x:Name="sp_status_bar_zoom" Margin="0,0,12,0" Orientation="Horizontal">
				<TextBlock x:Uid="tk_status_bar_zoom" Text="Zoom:"/>
