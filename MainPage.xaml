					x:Name="dialog_combo_box_0" x:Uid="dialog_combo_box_0" Header="?"
					Margin="0,0,0,4" 
					RelativePanel.AlignLeftWithPanel="True" RelativePanel.Below="dialog_check_box"
					Visibility="Collapsed" />
				<ComboBox
					x:Name="dialog_combo_box_1" x:Uid="dialog_combo_box_1" Header="?"
					Margin="0,0,0,4" 
					RelativePanel.AlignLeftWithPanel="True" RelativePanel.Below="dialog_combo_box_0" Visibility="Collapsed" />
				<muxc:RadioButtons
					x:Name="dialog_radio_btns" MaxColumns="2" Margin="0,0,0,4"
					RelativePanel.AlignLeftWithPanel="True" RelativePanel.Below="dialog_combo_box_1" Visibility="Collapsed" >
					<!--
					x:String などの UIElement でない要素のリストでは,
					オブザベーションのタイミングかなんかでうまくチェックがつかなかったりする.
					そこで UIElement であるラディオボタンを要素にする. 重複して使用する例については
					https://learn.microsoft.com/ja-jp/windows/apps/design/controls/radio-button を参照
					-->
					<RadioButton x:Name="dialog_radio_btn_0"/>
					<RadioButton x:Name="dialog_radio_btn_1"/>
				</muxc:RadioButtons>
			</RelativePanel>
		</ContentDialog>
		<!-- 印刷のための透明なキャンバス -->
		<Canvas x:Name="PrintCanvas" Opacity="0" />
	</Grid>
</Page>
