            <ListView x:Name="lv_summary" IsItemClickEnabled="False" Loaded="lv_summary_loaded" Margin="4,4" RelativePanel.AlignBottomWithPanel="True"  RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" RelativePanel.Below="btn_summary_close" SelectionChanged="lv_summary_selection_changed" SelectionMode="Extended" >
                <!--<ListView x:Name="lv_summary" IsItemClickEnabled="False" Loaded="lv_summary_loaded" Margin="4,4" RelativePanel.AlignBottomWithPanel="True"  RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" RelativePanel.Below="btn_summary_close" SelectionChanged="lv_summary_selection_changed" SelectionMode="Extended" >-->
                <ItemsControl.ItemTemplate>
                    <DataTemplate x:DataType="local:Summary">
                        <StackPanel Orientation="Horizontal">
                            <PathIcon Data="{x:Bind Data}" Height="32" Width="32" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <StackPanel >
                                <TextBlock Style="{ThemeResource BodyTextBlockStyle}" Text="{x:Bind Name, Mode=OneWay}"/>
                                <TextBlock Style="{ThemeResource CaptionTextBlockStyle}" Text="{x:Bind Hex, Mode=OneWay}"/>
                            </StackPanel>
                        </StackPanel>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ListView>
        </RelativePanel>
        <!-- ページパネル -->
        <RelativePanel x:Name="rp_page" Grid.Row="1" Grid.Column="0" >
            <SwapChainPanel
                x:Name="scp_page_panel"
                Loaded="scp_page_panel_loaded"
                KeyboardAcceleratorPlacementMode="Hidden"
                PointerCanceled="scp_pointer_canceled"
                PointerEntered="scp_pointer_entered"
                PointerExited="scp_pointer_exited"
                PointerMoved="scp_pointer_moved"
                PointerPressed="scp_pointer_pressed"
                PointerReleased="scp_pointer_released"
                PointerWheelChanged="scp_pointer_wheel_changed"                
                RelativePanel.AlignLeftWithPanel="True"
                RelativePanel.AlignTopWithPanel="True"
                RelativePanel.AlignRightWithPanel="True"
                RelativePanel.AlignBottomWithPanel="True"
                SizeChanged="scp_page_panel_size_changed"
                >
                <!--
                通常は, MenuFlyoutItem でキーボードアクセラレータを指定すべきだが,
                そうすると SwapChainPanel をクリックしたあとアクセラレーターが機能しなくなる.
                フォーカスのせいかもしれないが, 原因は不明.
                そこでこのエレメントにアクセラレーターを設定し Invoke する.
                通常は, ScopeOwner をすぐ上の親に限定すべきだが, 
                そうすると, 親より上のオブジェクトは既存ではなく, 新規に作成
                されてしまう。つまり, メンバ変数にアクセスできない。
                -->
                <SwapChainPanel.KeyboardAccelerators>
                    <KeyboardAccelerator Invoked="ka_range_next_invoked" Key="Down" Modifiers="Shift" />
                    <KeyboardAccelerator Invoked="ka_range_prev_invoked" Key="Up" Modifiers="Shift" />
                    <KeyboardAccelerator Invoked="ka_tool_select_invoked" Key="Escape" />
                    <KeyboardAccelerator Invoked="ka_select_prev_invoked" Key="Up" />
                    <KeyboardAccelerator Invoked="ka_select_next_invoked" Key="Down" />
                    <!--
                    <KeyboardAccelerator Invoked="ka_select_all_invoked" Key="A" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_copy_invoked" Key="C" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_delete_invoked" Key="Delete" />
                    <KeyboardAccelerator Invoked="ka_edit_text_invoked" Key="E" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_bring_to_front_invoked" Key="End" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_bring_forward_invoked" Key="PageDown" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_find_text_invoked" Key="F" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_group_invoked" Key="G" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_send_to_back_invoked" Key="Home" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_send_backward_invoked" Key="PageUp" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_summaty_invoked" Key="L" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_new_invoked" Key="N" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_zoom_reset_invoked" Key="Number0" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_zoom_reset_invoked" Key="NumberPad0" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_open_invoked" Key="O" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_save_invoked" Key="S" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_save_as_invoked" Key="S" Modifiers="Control,Shift" />
                    <KeyboardAccelerator Invoked="ka_ungroup_invoked" Key="U" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_paste_invoked" Key="V" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_cut_invoked" Key="X" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_redo_invoked" Key="Y" Modifiers="Control" />
                    <KeyboardAccelerator Invoked="ka_undo_invoked" Key="Z" Modifiers="Control" />
                    -->
                </SwapChainPanel.KeyboardAccelerators>
            </SwapChainPanel>
            <ScrollBar x:Name="sb_horz" Height="Auto" IndicatorMode="MouseIndicator" IsEnabled="True" Margin="0,0,16,0" Orientation="Horizontal" RelativePanel.AlignBottomWithPanel="True" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" Scroll="sb_scroll" Visibility="Visible" Width="Auto" />
            <ScrollBar x:Name="sb_vert" Height="Auto" IndicatorMode="MouseIndicator" IsEnabled="True" Margin="0,0,0,16" Orientation="Vertical" RelativePanel.AlignBottomWithPanel="true" RelativePanel.AlignRightWithPanel="True" RelativePanel.AlignTopWithPanel="True" Scroll="sb_scroll" Visibility="Visible" Width="Auto" />
        </RelativePanel>
        <!-- ステータスバー -->
        <!--
        <StackPanel x:Name="sp_stat_bar" Background="{ThemeResource SystemChromeHighColor}" Grid.Row="2" Orientation="Horizontal" Padding="8,0,8,0" >
        -->
        <StackPanel x:Name="sp_stat_bar" Background="{ThemeResource SystemControlBackgroundBaseLowBrush}" Grid.Row="2" Orientation="Horizontal" Padding="8,0,8,0" >
            <!-- ステータスバーコンテキストメニュー -->
            <StackPanel.ContextFlyout>
                <MenuFlyout>
                    <ToggleMenuFlyoutItem x:Name="tmfi_stat_tool_2" x:Uid="tmfi_stat_tool" Click="mi_stat_bar_click" Text="Tool" />
                    <ToggleMenuFlyoutItem x:Name="tmfi_stat_unit_2" x:Uid="tmfi_stat_unit" Click="mi_stat_bar_click" Text="Unit" />
                    <ToggleMenuFlyoutItem x:Name="tmfi_stat_curs_2" x:Uid="tmfi_stat_curs" Click="mi_stat_bar_click" Text="Cursor Position" />
                    <ToggleMenuFlyoutItem x:Name="tmfi_stat_grid_2" x:Uid="tmfi_stat_grid" Click="mi_stat_bar_click" Text="Grid Size" />
                    <ToggleMenuFlyoutItem x:Name="tmfi_stat_page_2" x:Uid="tmfi_stat_page" Click="mi_stat_bar_click" Text="Page Size" />
                    <ToggleMenuFlyoutItem x:Name="tmfi_stat_zoom_2" x:Uid="tmfi_stat_zoom" Click="mi_stat_bar_click" Text="Scale" />
                </MenuFlyout>
            </StackPanel.ContextFlyout>
            <StackPanel x:Name="sp_stat_tool" Margin="0,0,4,0" Orientation="Horizontal">
                <TextBlock Margin="0,0,0,0" Text="t:"/>
                <PathIcon x:Name="pi_tool" Data="M0,0" VerticalAlignment="Center" Width="16" Height="16">
                    <PathIcon.RenderTransform>
                        <ScaleTransform CenterX="8" CenterY="8" ScaleX="0.75" ScaleY="0.75" />
                    </PathIcon.RenderTransform>
                </PathIcon>
            </StackPanel>
            <TextBlock x:Name="tk_stat_unit" Margin="0,0,4,0" Text="0"/>
            <TextBlock x:Name="tk_stat_pos_x" Margin="0,0,4,0" Text="0"/>
            <TextBlock x:Name="tk_stat_pos_y" Margin="0,0,4,0" Text="0"/>
            <TextBlock x:Name="tk_stat_grid" Margin="0,0,4,0"  Text="0" />
            <TextBlock x:Name="tk_stat_width" Margin="0,0,4,0" Text="0"/>
            <TextBlock x:Name="tk_stat_height" Margin="0,0,4,0" Text="0"/>
            <TextBlock x:Name="tk_stat_zoom" Margin="0,0,4,0" Text="0"/>
            <TextBlock x:Name="tk_stat_cnt" Margin="0,0,4,0" Text="0"/>
        </StackPanel>
        <!-- 文字列入力ダイアログ -->
        <ContentDialog x:Name="cd_edit_text" x:Uid="cd_edit_text" DefaultButton="Primary" CloseButtonText="Cancel" PrimaryButtonText="Ok">
            <StackPanel>
                <TextBox x:Name="tx_edit" AcceptsReturn="True" Height="128" HorizontalAlignment="Stretch" PlaceholderText="Text" ScrollViewer.VerticalScrollBarVisibility="Auto" TextWrapping="Wrap" />
                <CheckBox x:Name="ck_ignore_blank" x:Uid="ck_ignore_blank" Content="Ignore bottom blank line." Margin="4,4" />
            </StackPanel>
        </ContentDialog>
        <!-- 保存確認ダイアログ -->
        <ContentDialog x:Name="cd_conf_save" x:Load="False" x:Uid="cd_conf_save" DefaultButton="Close">
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" >
                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="&#xE946;" Margin="0,0,4,0" />
                <TextBlock x:Uid="tk_conf_save"/>
            </StackPanel>
        </ContentDialog>
        <!-- 見つからないメッセージダイアログ -->
        <!--
        <ContentDialog x:Name="cd_not_found" x:Load="False" CloseButtonText="Ok" Closed="cd_message_closed" DefaultButton="Close" >
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" >
                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="&#xE946;" Margin="0,0,4,0" />
                <TextBlock x:Uid="tk_not_found" Text="No search text was found." />
            </StackPanel>
        </ContentDialog>
        -->
        <!-- アクセス拒否メッセージダイアログ -->
        <!--
        <ContentDialog x:Name="cd_acc_denied" x:Load="False" CloseButtonText="Ok" Closed="cd_message_closed" DefaultButton="Close">
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" >
                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="&#xEA39;" Margin="0,0,4,0" />
                <TextBlock x:Uid="tk_acc_denied" Text="Access is denied." />
            </StackPanel>
        </ContentDialog>
        -->
        <!-- 書体使用不可メッセージダイアログ -->
        <!--
        <ContentDialog x:Name="cd_unavailable_fonts" x:Load="False" CloseButtonText="Ok" Closed="cd_message_closed" DefaultButton="Close">
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" >
                <FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="&#xE946;" Margin="0,0,4,0" />
                <TextBlock x:Uid="tk_unavailable_fonts" Text="Document uses unavailable fonts." />
            </StackPanel>
        </ContentDialog>
        -->
        <!-- ページの寸法入力ダイアログ -->
        <ContentDialog x:Name="cd_page_size" x:Uid="cd_page_size" CloseButtonText="Close" DefaultButton="Close" PrimaryButtonClick="cd_page_size_pri_btn_click" PrimaryButtonText="Apply" SecondaryButtonClick="cd_page_size_sec_btn_click" SecondaryButtonText="Adjust to Shapes">
            <StackPanel >
                <TextBox x:Name="tx_page_width" x:Uid="tx_page_width" Margin="0,0,0,4" HorizontalAlignment="Stretch" VerticalAlignment="Top" Header="Width" TextChanged="tx_page_size_text_changed" />
                <TextBox x:Name="tx_page_height" x:Uid="tx_page_height" Margin="0,0,0,4" HorizontalAlignment="Stretch" VerticalAlignment="Top" Header="Height" TextChanged="tx_page_size_text_changed" />
                <TextBlock x:Name="tk_page_unit" x:Uid="tk_page_unit" Margin="0,0,0,4" HorizontalAlignment="Stretch" VerticalAlignment="Top" Text="Unit of distance"/>
            </StackPanel>
        </ContentDialog>
        <!-- ページの単位と書式ダイアログ -->
        <ContentDialog x:Name="cd_page_unit" x:Uid="cd_page_unit" CloseButtonText="Close" DefaultButton="Close" PrimaryButtonClick="cd_page_unit_pri_btn_click" PrimaryButtonText="Apply" >
            <StackPanel>
                <ComboBox x:Name="cx_page_unit" x:Uid="cx_page_unit" Header="Distance unit" Margin="0,0,0,4" >
                    <ComboBoxItem x:Uid="cxi_unit_pixel" Content="Pixel" />
                    <ComboBoxItem x:Uid="cxi_unit_inch" Content="Inch" />
                    <ComboBoxItem x:Uid="cxi_unit_milli" Content="Millimeter" />
                    <ComboBoxItem x:Uid="cxi_unit_point" Content="Point" />
                    <ComboBoxItem x:Uid="cxi_unit_grid" Content="Grid" />
                </ComboBox>
                <ComboBox x:Name="cx_color_style" x:Uid="cx_color_style" Header="Color component format" >
                    <ComboBoxItem x:Uid="cxi_color_dec" Content="Decimal" />
                    <ComboBoxItem x:Uid="cxi_color_hex" Content="Hexadecimal" />
                    <ComboBoxItem x:Uid="cxi_color_flt" Content="Real number" />
                    <ComboBoxItem x:Uid="cxi_color_cen" Content="Percent" />
                </ComboBox>
            </StackPanel>
        </ContentDialog>
        <!-- 属性入力ダイアログ -->
        <ContentDialog x:Name="cd_samp" x:Load="False" CloseButtonText="Close" DefaultButton="Close" Opened="cd_samp_opened" PrimaryButtonText="Apply" >
            <RelativePanel>
                <SwapChainPanel
                    x:Name="scp_samp_panel"
                    Height="170"
                    Margin="0,0,0,4"
                    MaxHeight="170"
                    MaxWidth="170"
                    RelativePanel.AlignTopWithPanel="True"
                    RelativePanel.AlignHorizontalCenterWithPanel="True"
                    Width="170"
                    />
                <!-- Height を指定するこれ大事 -->
                <ListView x:Name="lv_font_stretch" Loaded="lv_font_stretch_loaded" RelativePanel.Below="scp_samp_panel" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" RelativePanel.AlignBottomWithPanel="True" Visibility="Collapsed" Width="256">
                    <ListViewItem x:Uid="lvi_stretch_undefined" Content="Undefined" />
                    <ListViewItem x:Uid="lvi_stretch_ultra_condensed" Content="Ultra Condensed" />
                    <ListViewItem x:Uid="lvi_stretch_extra_condensed" Content="Extra Condensed" />
                    <ListViewItem x:Uid="lvi_stretch_condensed" Content="Condensed" />
                    <ListViewItem x:Uid="lvi_stretch_semi_condensed" Content="Semi Condensed" />
                    <ListViewItem x:Uid="lvi_stretch_normal" Content="Normal" />
                    <ListViewItem x:Uid="lvi_stretch_semi_expanded" Content="Semi Expanded" />
                    <ListViewItem x:Uid="lvi_stretch_expanded" Content="Expanded" />
                    <ListViewItem x:Uid="lvi_stretch_extra_expanded" Content="Extra Expanded" />
                    <ListViewItem x:Uid="lvi_stretch_ultra_expanded" Content="Ultra Expanded" />
                </ListView>
                <ListView x:Name="lv_font_family" Loaded="lv_font_family_loaded" RelativePanel.Below="scp_samp_panel" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" RelativePanel.AlignBottomWithPanel="True" Visibility="Collapsed" Width="256" />
                <ListView x:Name="lv_font_weight" Loaded="lv_font_weight_loaded" RelativePanel.Below="scp_samp_panel" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" RelativePanel.AlignBottomWithPanel="True" Visibility="Collapsed" Width="256">
                    <ListViewItem x:Uid="lvi_weight_thin" Content="Thin" />
                    <ListViewItem x:Uid="lvi_weight_extra_light" Content="Extra Light" />
                    <ListViewItem x:Uid="lvi_weight_light" Content="Light" />
                    <ListViewItem x:Uid="lvi_weight_semi_light" Content="Semi Light" />
                    <ListViewItem x:Uid="lvi_weight_normal" Content="Normal" />
                    <ListViewItem x:Uid="lvi_weight_medium" Content="Medium" />
                    <ListViewItem x:Uid="lvi_weight_demi_bold" Content="Demi Bold" />
                    <ListViewItem x:Uid="lvi_weight_bold" Content="Bold" />
                    <ListViewItem x:Uid="lvi_weight_extra_bold" Content="Extra Bold" />
                    <ListViewItem x:Uid="lvi_weight_black" Content="Black" />
                    <ListViewItem x:Uid="lvi_weight_extra_black" Content="Extra Black" />
                </ListView>
                <Slider x:Name="slider0" x:Uid = "slider0" IsThumbToolTipEnabled="False" Margin="0,0,0,4" Maximum="255" Minimum="0" RelativePanel.Below="scp_samp_panel" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" StepFrequency="1" Visibility="Collapsed" />
                <Slider x:Name="slider1" x:Uid = "slider1" IsThumbToolTipEnabled="False" Margin="0,0,0,4" Maximum="255" Minimum="0" RelativePanel.Below="slider0" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" StepFrequency="1" Visibility="Collapsed" />
                <Slider x:Name="slider2" x:Uid = "slider2" IsThumbToolTipEnabled="False" Margin="0,0,0,4" Maximum="255" Minimum="0" RelativePanel.Below="slider1" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" StepFrequency="1" Visibility="Collapsed" />
                <Slider x:Name="slider3" x:Uid = "slider3" IsThumbToolTipEnabled="False" Margin="0,0,0,4" Maximum="255" Minimum="0" RelativePanel.Below="slider2" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignRightWithPanel="True" StepFrequency="1" Visibility="Collapsed" />
            </RelativePanel>
        </ContentDialog>
    </Grid>
</Page>
