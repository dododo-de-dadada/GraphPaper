<Page
	x:Class="GraphPaper.MainPage"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="using:GraphPaper"
	xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Loaded="Page_Loaded"
	mc:Ignorable="d">
	<!-- x:Name はコードビハインドのためのオブジェクト. x:Uid はリソースの参照. -->
	<Page.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Dictionary.xaml" />
			</ResourceDictionary.MergedDictionaries>
			<Style TargetType="FontIcon">
				<Setter Property="FontFamily" Value="{StaticResource SymbolThemeFontFamily}" />
			</Style>
			<Style TargetType="PathIcon">
				<Setter Property="HorizontalAlignment" Value="Center" />
				<Setter Property="VerticalAlignment" Value="Center" />
				<Setter Property="Height" Value="16" />
				<Setter Property="Width" Value="16" />
			</Style>
			<!-- 右クリックのポップアップメニュー -->
			<MenuFlyout x:Name="popup_menu">
				<!-- 元に戻す -->
				<MenuFlyoutItem x:Name="popup_undo" x:Uid="menu_undo" Click="undo_click" Icon="Undo" KeyboardAcceleratorTextOverride="Ctrl+Z" Text="Undo"  >
					<MenuFlyoutItem.KeyboardAccelerators>
						<!--<KeyboardAccelerator Key="Z" Modifiers="Control" Invoked="undo_invoked" />-->
						<KeyboardAccelerator Key="Z" Modifiers="Control"/>
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- やり直し -->
				<MenuFlyoutItem x:Name="popup_redo" x:Uid="menu_redo" Click="redo_click" Icon="Redo" KeyboardAcceleratorTextOverride="Ctrl+Y" Text="Redo" >
					<MenuFlyoutItem.KeyboardAccelerators>
						<!--<KeyboardAccelerator Key="Y" Modifiers="Control" Invoked="redo_invoked" />-->
						<KeyboardAccelerator Key="Y" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- ================ -->
				<MenuFlyoutSeparator x:Name="popup_sepa_undo_xcvd"/>
				<!-- 切り取り -->
				<MenuFlyoutItem x:Name="popup_cut" x:Uid="menu_cut" Click="cut_click_async" Icon="Cut" KeyboardAcceleratorTextOverride="Ctrl+X" Text="Cut" >
					<MenuFlyoutItem.KeyboardAccelerators>
						<!--<KeyboardAccelerator Key="X" Modifiers="Control" Invoked="cut_invoked" />-->
						<KeyboardAccelerator Key="X" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- コピー -->
				<MenuFlyoutItem x:Name="popup_copy" x:Uid="menu_copy" Click="copy_click_async" KeyboardAcceleratorTextOverride="Ctrl+C" Icon="Copy" Text="Copy">
				</MenuFlyoutItem>
				<!-- 貼り付け -->
				<MenuFlyoutItem x:Name="popup_paste" x:Uid="menu_paste" Click="paste_click" Icon="Paste" KeyboardAcceleratorTextOverride="Ctrl+V" Text="Paste" >
					<MenuFlyoutItem.KeyboardAccelerators>
						<!--<KeyboardAccelerator Key="V" Modifiers="Control" Invoked="paste_invoked" />-->
						<KeyboardAccelerator Key="V" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- 削除 -->
				<MenuFlyoutItem x:Name="popup_delete" x:Uid="menu_delete" Click="delete_click" Icon="Delete" KeyboardAcceleratorTextOverride="Del" Text="Delete">
					<MenuFlyoutItem.KeyboardAccelerators>
						<!--<KeyboardAccelerator Key="Delete" Invoked="delete_invoked" />
						<KeyboardAccelerator Key="D" Modifiers="Control" Invoked="delete_invoked" />-->
						<KeyboardAccelerator Key="Delete" />
						<KeyboardAccelerator Key="D" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- ================ -->
				<MenuFlyoutSeparator />
				<!-- すべて選択 -->
				<MenuFlyoutItem x:Name="popup_select_all" x:Uid="menu_select_all" Click="select_all_click" Icon="SelectAll" KeyboardAcceleratorTextOverride="Ctrl+A" Text="Select All">
					<MenuFlyoutItem.KeyboardAccelerators>
						<!--<KeyboardAccelerator Key="A" Modifiers="Control" Invoked="select_all_invoked" />-->
						<KeyboardAccelerator Key="A" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- 並び替え -->
				<MenuFlyoutSubItem x:Name="popup_order" x:Uid="menu_order" Text="Order">
					<MenuFlyoutSubItem.Icon>
						<FontIcon Glyph="{StaticResource glyph_order}" />
					</MenuFlyoutSubItem.Icon>
					<!-- 最前面に移動 -->
					<MenuFlyoutItem x:Name="popup_bring_to_front" x:Uid="menu_bring_to_front" Click="bring_to_front_click" KeyboardAcceleratorTextOverride="Ctrl+End" Text="Bring to Front" >
						<MenuFlyoutItem.KeyboardAccelerators>
							<!--<KeyboardAccelerator Key="End" Modifiers="Control" Invoked="bring_to_front_invoked"/>-->
							<KeyboardAccelerator Key="End" Modifiers="Control"/>
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
					<!--  ひとつ前面に移動 -->
					<MenuFlyoutItem x:Name="popup_bring_forward" x:Uid="menu_bring_forward" Click="bring_forward_click" KeyboardAcceleratorTextOverride="Ctrl+PgDown" Text="Bring Forward">
						<MenuFlyoutItem.KeyboardAccelerators>
							<!--<KeyboardAccelerator Key="PageDown" Modifiers="Control" Invoked="bring_forward_invoked"/>-->
							<KeyboardAccelerator Key="PageDown" Modifiers="Control"/>
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
					<!--  ひとつ背面に移動 -->
					<MenuFlyoutItem x:Name="popup_send_backward" x:Uid="menu_send_backward" Click="send_backward_click" KeyboardAcceleratorTextOverride="Ctrl+PgUp" Text="Send Backward" >
						<MenuFlyoutItem.KeyboardAccelerators>
							<!--<KeyboardAccelerator Key="PageUp" Modifiers="Control" Invoked="send_backward_invoked"/>-->
							<KeyboardAccelerator Key="PageUp" Modifiers="Control"/>
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
					<!-- 最背面に移動 -->
					<MenuFlyoutItem x:Name="popup_send_to_back" x:Uid="menu_send_to_back" Click="send_to_back_click" KeyboardAcceleratorTextOverride="Ctrl+Home" Text="Send to Back" >
						<MenuFlyoutItem.KeyboardAccelerators>
							<!--<KeyboardAccelerator Key="Home" Modifiers="Control" Invoked="send_to_back_invoked"/>-->
							<KeyboardAccelerator Key="Home" Modifiers="Control"/>
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
				</MenuFlyoutSubItem>
				<!-- ================ -->
				<MenuFlyoutSeparator/>
				<!-- グループ化 -->
				<MenuFlyoutItem x:Name="popup_group" x:Uid="menu_group" Click="group_click" KeyboardAcceleratorTextOverride="Ctrl+G" Text="Group" >
					<MenuFlyoutItem.Icon>
						<PathIcon Data="{StaticResource data_group}" />
					</MenuFlyoutItem.Icon>
					<MenuFlyoutItem.KeyboardAccelerators>
						<!--<KeyboardAccelerator Key="G" Modifiers="Control" Invoked="group_invoked" />-->
						<KeyboardAccelerator Key="G" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- グループを解除 -->
				<MenuFlyoutItem x:Name="popup_ungroup" x:Uid="menu_ungroup" Click="ungroup_click" KeyboardAcceleratorTextOverride="Ctrl+U" Text="Ungroup" >
					<MenuFlyoutItem.Icon>
						<PathIcon Data="{StaticResource data_ungroup}" />
					</MenuFlyoutItem.Icon>
					<MenuFlyoutItem.KeyboardAccelerators>
						<!--<KeyboardAccelerator Key="U" Modifiers="Control" Invoked="ungroup_invoked" />-->
						<KeyboardAccelerator Key="U" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- ================ -->
				<MenuFlyoutSeparator/>
				<!-- 文字列の検索/置換 -->
				<MenuFlyoutItem x:Name="popup_find_text" x:Uid="menu_find_text" Click="find_text_click" Icon="Find" KeyboardAcceleratorTextOverride="Ctrl+F" Text="Find/Replace Text">
					<MenuFlyoutItem.KeyboardAccelerators>
						<!--<KeyboardAccelerator Key="F" Modifiers="Control" Invoked="find_text_invoked"/>-->
						<KeyboardAccelerator Key="F" Modifiers="Control"/>
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- ================ -->
                <MenuFlyoutSeparator/>
                <MenuFlyoutSubItem x:Name="popup_edit_shape" x:Uid="menu_edit_shape" Text="Edit Shape">
					<!-- パスの方向を反転 -->
					<MenuFlyoutItem x:Name="popup_reverse_path" x:Uid="menu_reverse_path" Click="reverse_path_click" Icon="Switch" Text="Reverse Path Direction" />
					<!-- 反時計周りに円弧を描く -->
					<MenuFlyoutItem x:Name="popup_draw_arc_ccw" x:Uid="menu_draw_arc_ccw" Click="draw_arc_direction_click" Text="Draw Arc Counter Clockwise">
						<MenuFlyoutItem.Icon>
							<PathIcon Data="{StaticResource data_arc_ccw}" />
						</MenuFlyoutItem.Icon>
					</MenuFlyoutItem>
					<!-- 時計周りに円弧を描く -->
					<MenuFlyoutItem x:Name="popup_draw_arc_cw" x:Uid="menu_draw_arc_cw" Click="draw_arc_direction_click" Text="Draw Arc Clockwise">
						<MenuFlyoutItem.Icon>
							<PathIcon Data="{StaticResource data_arc_clockwise}" />
						</MenuFlyoutItem.Icon>
					</MenuFlyoutItem>
					<!-- 多角形を開く -->
                    <MenuFlyoutItem x:Name="popup_open_polygon" x:Uid="menu_open_polygon" Click="open_or_close_poly_end_click" Text="Open Polygon">
						<MenuFlyoutItem.Icon>
							<PathIcon Data="{StaticResource data_polyline}" />
						</MenuFlyoutItem.Icon>
					</MenuFlyoutItem>
					<!-- 折れ線を閉じる -->
                    <MenuFlyoutItem x:Name="popup_close_polyline" x:Uid="menu_close_polyline" Click="open_or_close_poly_end_click" Text="Close Polyline">
						<MenuFlyoutItem.Icon>
							<PathIcon Data="{StaticResource data_polygon}" />
						</MenuFlyoutItem.Icon>
					</MenuFlyoutItem>
					<!-- 画像を原画像に戻す -->
					<MenuFlyoutItem x:Name="popup_revert_image" x:Uid="menu_revert_image" Click="revert_image_click" Text="Revert Image to Original" >
						<MenuFlyoutItem.Icon>
							<FontIcon Glyph="{StaticResource glyph_update_restore}" />
						</MenuFlyoutItem.Icon>
					</MenuFlyoutItem>
					<!-- 文字列の編集 -->
					<!--
					<MenuFlyoutItem x:Name="mfi_popup_edit_text" x:Uid="mfi_menu_edit_text" Click="meth_text_edit_click" Text="Edit Text" >
						<MenuFlyoutItem.Icon>
							<FontIcon Glyph="{StaticResource glyph_ibeam}"/>
						</MenuFlyoutItem.Icon>
						<MenuFlyoutItem.KeyboardAccelerators>
							<KeyboardAccelerator Key="E" Modifiers="Control" />
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
					-->
                </MenuFlyoutSubItem>
				<MenuFlyoutSubItem x:Name="popup_stroke" Text="Stroke">
					<!-- 破線の形式 -->
					<MenuFlyoutSubItem x:Name="mfsi_popup_stroke_dash" x:Uid="mfsi_menu_stroke_dash" Text="Style">
						<MenuFlyoutSubItem.Icon>
							<PathIcon Data="{StaticResource data_stroke_dash}" />
						</MenuFlyoutSubItem.Icon>
						<!-- 実線 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_dash_solid" x:Uid="rmfi_menu_stroke_dash_solid" GroupName="gn_popup_stroke_dash" Click="stroke_dash_click" IsChecked="True" Text="Solid" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_line}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- ================ -->
						<MenuFlyoutSeparator />
						<!-- 破線 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_dash_dash" x:Uid="rmfi_menu_stroke_dash_dash" GroupName="gn_popup_stroke_dash" Click="stroke_dash_click" Text="Dash">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_dash}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 点線 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_dash_dot" x:Uid="rmfi_menu_stroke_dash_dot" GroupName="gn_popup_stroke_dash" Click="stroke_dash_click" Text="Dot">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_dot}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 一点破線 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_dash_dash_dot" x:Uid="rmfi_menu_stroke_dash_dash_dot" GroupName="gn_popup_stroke_dash" Click="stroke_dash_click" Text="Dash Dot">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_dash_dot}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 二点破線 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_dash_dash_dot_dot" x:Uid="rmfi_menu_stroke_dash_dash_dot_dot" GroupName="gn_popup_stroke_dash" Click="stroke_dash_click" Text="Dash Dot Dot">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_dash_dot_dot}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
					</MenuFlyoutSubItem>
					<!-- 破線の配置 -->
					<MenuFlyoutItem x:Name="mfi_popup_stroke_dash_pat" x:Uid="mfi_menu_stroke_dash_pat" Click="stroke_dash_pat_click_async" IsEnabled="False" Text="Dash Pattern..." />
					<!-- 線の太さ -->
					<MenuFlyoutSubItem x:Name="mfsi_popup_stroke_width" x:Uid="mfsi_stroke_width" Text="Stroke Width" >
						<MenuFlyoutSubItem.Icon>
							<PathIcon Data="{StaticResource data_stroke_width}" />
						</MenuFlyoutSubItem.Icon>
						<!-- 0px -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_width_0px" x:Uid="rmfi_menu_stroke_width_0px" GroupName="gn_popup_stroke_width" Click="stroke_width_click" IsChecked="true" Text="0 px" />
						<!-- 1px -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_width_1px" x:Uid="rmfi_menu_stroke_width_1px" GroupName="gn_popup_stroke_width" Click="stroke_width_click" IsChecked="true" Text="1 px" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_stroke_width_1px}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 2px -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_width_2px" x:Uid="rmfi_menu_stroke_width_2px" GroupName="gn_popup_stroke_width" Click="stroke_width_click" Text="2 px" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_popup_stroke_widthpx}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 3px -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_width_3px" x:Uid="rmfi_menu_stroke_width_3px" GroupName="gn_popup_stroke_width" Click="stroke_width_click" Text="3 px" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_stroke_width_3px}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 4px -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_width_4px" x:Uid="rmfi_menu_stroke_width_4px" GroupName="gn_popup_stroke_width" Click="stroke_width_click" Text="4 px" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_stroke_width_4px}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 8px -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_width_8px" x:Uid="rmfi_menu_stroke_width_8px" GroupName="gn_popup_stroke_width" Click="stroke_width_click" Text="8 px" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_stroke_width_8px}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 12px -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_width_12px" x:Uid="rmfi_menu_stroke_width_12px" GroupName="gn_popup_stroke_width" Click="stroke_width_click" Text="8 px" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_stroke_width_12px}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 16px -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_width_16px" x:Uid="rmfi_menu_stroke_width_16px" GroupName="gn_popup_stroke_width" Click="stroke_width_click" Text="16 px" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_stroke_width_16px}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- ================ -->
						<!-- その他 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_width_other" x:Uid="rmfi_menu_stroke_width_other" GroupName="gn_popup_stroke_width" Click="stroke_width_click_async" Text="Others..." />
					</MenuFlyoutSubItem>
					<!-- 線の結合の形式 -->
					<MenuFlyoutSubItem x:Name="mfsi_popup_stroke_join" x:Uid="mfsi_menu_stroke_join" Text="Join Style ">
						<MenuFlyoutSubItem.Icon>
							<PathIcon Data="{StaticResource data_join_bevel}" />
						</MenuFlyoutSubItem.Icon>
						<!-- 尖り -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_join_miter_or_bevel" x:Uid="rmfi_menu_stroke_join_miter_or_bevel" GroupName="gn_popup_stroke_join" Click="stroke_join_click" Text="Miter" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_join_miter}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 丸まり -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_join_round" x:Uid="rmfi_menu_stroke_join_round" GroupName="gn_popup_stroke_join" Click="stroke_join_click" Text="Rounded" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_join_round}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 面取り -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_join_bevel" x:Uid="rmfi_menu_stroke_join_bevel" GroupName="gn_popup_stroke_join" Click="stroke_join_click" Text="Beveled" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_join_bevel}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 尖り (使用不可) -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_join_miter" x:Uid="rmfi_menu_stroke_join_miter" GroupName="gn_popup_stroke_join" Click="stroke_join_click" Text="Miter" Visibility="Collapsed">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_join_miter}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<MenuFlyoutSeparator />
						<!-- 尖り制限 -->
						<MenuFlyoutItem x:Name="mfi_popup_stroke_join_limit" x:Uid="mfi_menu_stroke_join_limit" Click="stroke_join_limit_click_async" Text="Miter Limit..." />
					</MenuFlyoutSubItem>
					<!-- 端の形式 -->
					<MenuFlyoutSubItem x:Name="mfsi_popup_stroke_cap" x:Uid="mfsi_menu_stroke_cap" Text="Cap Style ">
						<MenuFlyoutSubItem.Icon>
							<PathIcon Data="{StaticResource data_cap_square}" />
						</MenuFlyoutSubItem.Icon>
						<!-- 平面 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_cap_flat" x:Uid="rmfi_menu_stroke_cap_flat" GroupName="gn_popup_stroke_cap" Click="stroke_cap_click" IsChecked="true" Text="Flat">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_cap_flat}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 正方形 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_cap_square" x:Uid="rmfi_menu_stroke_cap_square" GroupName="gn_popup_stroke_cap"  Click="stroke_cap_click" Text="Square" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_cap_square}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 円形 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_cap_round" x:Uid="rmfi_menu_stroke_cap_round" GroupName="gn_popup_stroke_cap" Click="stroke_cap_click" Text="Round" >
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_cap_round}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- 三角形 -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_cap_triangle" x:Uid="rmfi_menu_stroke_cap_triangle" GroupName="gn_popup_stroke_cap" Click="stroke_cap_click" Text="Triangle">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_cap_triangle}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
					</MenuFlyoutSubItem>
					<!-- 矢じるしの形式 -->
					<MenuFlyoutSubItem x:Name="mfsi_popup_stroke_arrow" x:Uid="mfsi_menu_stroke_arrow" Text="Arrowhead Style">
						<MenuFlyoutSubItem.Icon>
							<PathIcon Data="{StaticResource data_arrow_opened}" />
						</MenuFlyoutSubItem.Icon>
						<!-- なし -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_arrow_none" x:Uid="rmfi_menu_stroke_arrow_none" Click="stroke_arrow_click" IsChecked="True" Text="None" GroupName="gn_popup_stroke_arrow">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_line}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<!-- ================ -->
						<MenuFlyoutSeparator />
						<!-- 開いた矢じるし -->
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_arrow_opened" x:Uid="rmfi_menu_stroke_arrow_opened" Click="stroke_arrow_click" Text="Opened" GroupName="gn_popup_stroke_arrow">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_arrow_opened}" />
							</muxc:RadioMenuFlyoutItem.Icon>
						</muxc:RadioMenuFlyoutItem>
						<muxc:RadioMenuFlyoutItem x:Name="rmfi_popup_stroke_arrow_filled" x:Uid="rmfi_menu_stroke_arrow_filled" Click="stroke_arrow_click" Text="Filled" GroupName="gn_popup_stroke_arrow">
							<muxc:RadioMenuFlyoutItem.Icon>
								<PathIcon Data="{StaticResource data_arrow_filled}" />
