<Page
	x:Class="GraphPaper.MainPage"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="using:GraphPaper"
	xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Loaded="Page_Loaded"
	mc:Ignorable="d">
	<!-- x:Name はコードビハインドのためのオブジェクト. x:Uid はリソースの参照. -->
	<Page.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Dictionary.xaml" />
			</ResourceDictionary.MergedDictionaries>
			<Style TargetType="FontIcon">
				<Setter Property="FontFamily" Value="{StaticResource SymbolThemeFontFamily}" />
			</Style>
			<Style TargetType="PathIcon">
				<Setter Property="HorizontalAlignment" Value="Center" />
				<Setter Property="VerticalAlignment" Value="Center" />
				<Setter Property="Height" Value="16" />
				<Setter Property="Width" Value="16" />
			</Style>
			<MenuFlyout x:Name="mf_context">
				<MenuFlyoutItem
					x:Name="mfi_undo_2" x:Uid="mfi_undo"
					Click="ustack_undo_click"
					Icon="Undo" Text="Undo" IsEnabled="False" >
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="Z" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- やり直し -->
				<MenuFlyoutItem
					x:Name="mfi_redo_2" x:Uid="mfi_redo"
					Click="ustack_redo_click"
					Icon="Redo" Text="Redo" IsEnabled="False" >
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="Y" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- ================ -->
				<MenuFlyoutSeparator x:Name="mfs_separator_undo_xcvd"/>
				<!-- 切り取り -->
				<MenuFlyoutItem
					x:Name="mfi_xcvd_cut_2" x:Uid="mfi_xcvd_cut"
					Click="xcvd_cut_click_async"
					Icon="Cut" Text="Cut" IsEnabled="False" >
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="X" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- コピー -->
				<MenuFlyoutItem
					x:Name="mfi_xcvd_copy_2" x:Uid="mfi_xcvd_copy"
					Click="xcvd_copy_click_async"
					Icon="Copy" Text="Copy" IsEnabled="False">
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="C" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- 貼り付け -->
				<MenuFlyoutItem
					x:Name="mfi_xcvd_paste_2" x:Uid="mfi_xcvd_paste"
					Click="xcvd_paste_click"
					Icon="Paste" Text="Paste"  IsEnabled="False" >
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="V" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- 削除 -->
				<MenuFlyoutItem
					x:Name="mfi_xcvd_delete_2" x:Uid="mfi_xcvd_delete"
					Click="xcvd_delete_click"
					Icon="Delete" Text="Delete" IsEnabled="False" >
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="D" Modifiers="Control" />
						<KeyboardAccelerator Key="Delete" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- ================ -->
                <MenuFlyoutSeparator x:Name="mfs_separator_xcvd_select"/>
				<!-- すべて選択 -->
				<MenuFlyoutItem
					x:Name="mfi_select_all_2" x:Uid="mfi_select_all"
					Click="select_all_click"
					Icon="SelectAll" Text="Select All" IsEnabled="False">
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="A" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- 並び替え -->
				<MenuFlyoutSubItem
					x:Name="mfsi_order_2" x:Uid="mfsi_order" Text="Order">
					<MenuFlyoutSubItem.Icon>
						<FontIcon Glyph="{StaticResource glyph_order}" />
					</MenuFlyoutSubItem.Icon>
					<!-- 最前面に移動 -->
					<MenuFlyoutItem
						x:Name="mfi_bring_to_front_2" x:Uid="mfi_bring_to_front"
						Click="order_bring_to_front_click"
						Text="Bring to Front" IsEnabled="false">
						<MenuFlyoutItem.KeyboardAccelerators>
							<KeyboardAccelerator Key="End" Modifiers="Control" />
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
					<!--  ひとつ前面に移動 -->
					<MenuFlyoutItem
						x:Name="mfi_bring_forward_2" x:Uid="mfi_bring_forward"
						Click="order_bring_forward_click"
						IsEnabled="false" Text="Bring Forward">
						<MenuFlyoutItem.KeyboardAccelerators>
							<KeyboardAccelerator Key="PageDown" Modifiers="Control" />
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
					<!--  ひとつ背面に移動 -->
					<MenuFlyoutItem
						x:Name="mfi_send_backward_2" x:Uid="mfi_send_backward"
						Click="order_send_backward_click"
						Text="Send Backward" IsEnabled="false" >
						<MenuFlyoutItem.KeyboardAccelerators>
							<KeyboardAccelerator Key="PageUp" Modifiers="Control" />
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
					<!-- 最背面に移動 -->
					<MenuFlyoutItem
						x:Name="mfi_send_to_back_2" x:Uid="mfi_send_to_back"
						Click="order_send_to_back_click"
						Text="Send to Back" IsEnabled="false" >
						<MenuFlyoutItem.KeyboardAccelerators>
							<KeyboardAccelerator Key="Home" Modifiers="Control" />
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
				</MenuFlyoutSubItem>
				<!-- ================ -->
                <MenuFlyoutSeparator x:Name="mfs_separator_select_group"/>
				<!-- グループ化 -->
				<MenuFlyoutItem
					x:Name="mfi_group_2" x:Uid="mfi_group"
					Click="group_click" IsEnabled="false" Text="Group" >
					<MenuFlyoutItem.Icon>
						<PathIcon Data="{StaticResource data_group}" />
					</MenuFlyoutItem.Icon>
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="G" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- グループを解除 -->
				<MenuFlyoutItem
					x:Name="mfi_ungroup_2" x:Uid="mfi_ungroup"
					Click="ungroup_click" Text="Ungroup" IsEnabled="false" >
					<MenuFlyoutItem.Icon>
						<PathIcon Data="{StaticResource data_ungroup}" />
					</MenuFlyoutItem.Icon>
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="U" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- ================ -->
                <MenuFlyoutSeparator x:Name="mfs_separator_group_edit"/>
                <!-- 多角形の終端の編集 -->
				<MenuFlyoutSubItem
					x:Name="mfsi_edit_poly_end_2" x:Uid="mfsi_edit_poly_end" Text="Edit Polygon End">
					<MenuFlyoutSubItem.Icon>
						<PathIcon Data="{StaticResource data_end_open}" />
					</MenuFlyoutSubItem.Icon>
					<MenuFlyoutItem
						x:Name="mfi_edit_poly_open_2" x:Uid="mfi_edit_poly_open"
						Click="edit_poly_end_click"
						Text="Open Polygon End" >
						<MenuFlyoutItem.Icon>
							<PathIcon Data="{StaticResource data_end_open}" />
						</MenuFlyoutItem.Icon>
						<MenuFlyoutItem.KeyboardAccelerators>
							<KeyboardAccelerator Key="P" Modifiers="Control" />
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
					<MenuFlyoutItem
						x:Name="mfi_edit_poly_close_2" x:Uid="mfi_edit_poly_close"
						Click="edit_poly_end_click"
						Text="Close Polygon End" >
						<MenuFlyoutItem.Icon>
							<PathIcon Data="{StaticResource data_end_closed}" />
						</MenuFlyoutItem.Icon>
						<MenuFlyoutItem.KeyboardAccelerators>
							<KeyboardAccelerator Key="P" Modifiers="Control,Shift" />
						</MenuFlyoutItem.KeyboardAccelerators>
					</MenuFlyoutItem>
				</MenuFlyoutSubItem>
				<!-- 円弧の編集 -->
				<MenuFlyoutItem
					x:Name="mfi_edit_arc_2" x:Uid="mfi_edit_arc"
					Click="edit_arc_click_async"
					Text="Edit Arc" >
					<MenuFlyoutItem.Icon>
						<PathIcon Data="{StaticResource data_arc}" />
					</MenuFlyoutItem.Icon>
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="R" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- 文字列の編集 -->
				<MenuFlyoutItem
					x:Name="mfi_edit_text_2" x:Uid="mfi_edit_text"
					Click="edit_text_click_async"
					Text="Edit Text" >
					<MenuFlyoutItem.Icon>
						<FontIcon Glyph="{StaticResource glyph_ibeam}"/>
					</MenuFlyoutItem.Icon>
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="E" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<!-- 文字列の検索/置換 -->
				<MenuFlyoutItem
					x:Name="mfi_find_text_2" x:Uid="mfi_find_text"
					Click="find_text_click" Icon="Find" Text="Find/Replace Text">
					<MenuFlyoutItem.KeyboardAccelerators>
						<KeyboardAccelerator Key="F" Modifiers="Control" />
					</MenuFlyoutItem.KeyboardAccelerators>
				</MenuFlyoutItem>
				<MenuFlyoutItem
					x:Name="mfi_text_fit_frame_to_text_2" x:Uid="mfi_text_fit_frame_to_text"
					Click="text_fit_frame_to_text_click" Text="Fit Frame Size to Text" >
					<MenuFlyoutItem.Icon>
						<FontIcon Glyph="{StaticResource glyph_fit_page}"/>
					</MenuFlyoutItem.Icon>
				</MenuFlyoutItem>
				<!-- ================ -->
				<MenuFlyoutSeparator x:Name="mfs_separator_edit_stroke"/>
                <!-- 破線の形式 -->
                <MenuFlyoutSubItem
                    x:Name="mfsi_dash_style_2" x:Uid="mfsi_dash_style" Text="Style">
                    <MenuFlyoutSubItem.Icon>
                        <PathIcon Data="{StaticResource data_dash_style}" />
                    </MenuFlyoutSubItem.Icon>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_dash_style_solid_2" x:Uid="rmfi_dash_style_solid" GroupName="gn_dash_style"
						Click="dash_style_click" IsChecked="True" Text="Solid" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_line}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <MenuFlyoutSeparator />
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_dash_style_dash_2" x:Uid="rmfi_dash_style_dash" GroupName="gn_dash_style"
						Click="dash_style_click" Text="Dash">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_dash}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_dash_style_dot_2" x:Uid="rmfi_dash_style_dot" GroupName="gn_dash_style"
						Click="dash_style_click" Text="Dot">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_dot}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_dash_style_dash_dot_2" x:Uid="rmfi_dash_style_dash_dot" GroupName="gn_dash_style"
						Click="dash_style_click" Text="Dash Dot">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_dash_dot}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_dash_style_dash_dot_dot_2" x:Uid="rmfi_dash_style_dash_dot_dot" GroupName="gn_dash_style"
						Click="dash_style_click" Text="Dash Dot Dot">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_dash_dot_dot}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                </MenuFlyoutSubItem>
                <!-- 破線の配置 -->
                <MenuFlyoutItem
					x:Name="mfi_dash_pat_2" x:Uid="mfi_dash_pat" Click="dash_pat_click_async"
					IsEnabled="False" Text="Dash Pattern..." />
                <!-- 線の太さ -->
                <MenuFlyoutSubItem
                    x:Name="mfsi_stroke_width_2" x:Uid="mfsi_stroke_width" Text="Stroke Width" >
                    <MenuFlyoutSubItem.Icon>
                        <PathIcon Data="{StaticResource data_stroke_width}" />
                    </MenuFlyoutSubItem.Icon>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_stroke_width_0px_2" x:Uid="rmfi_stroke_width_0px" GroupName="gn_stroke_width"
						Click="stroke_width_click" IsChecked="true" Text="0 px" />
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_stroke_width_1px_2" x:Uid="rmfi_stroke_width_1px" GroupName="gn_stroke_width"
						Click="stroke_width_click" IsChecked="true" Text="1 px" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_stroke_width_1px}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_stroke_width_2px_2" x:Uid="rmfi_stroke_width_2px" GroupName="gn_stroke_width" 
						Click="stroke_width_click" Text="2 px" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_stroke_width_2px}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_stroke_width_3px_2" x:Uid="rmfi_stroke_width_3px" GroupName="gn_stroke_width" 
						Click="stroke_width_click" Text="3 px" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_stroke_width_3px}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_stroke_width_4px_2" x:Uid="rmfi_stroke_width_4px" GroupName="gn_stroke_width" 
						Click="stroke_width_click" Text="4 px" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_stroke_width_4px}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <MenuFlyoutSeparator />
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_stroke_width_other_2" x:Uid="rmfi_stroke_width_other" GroupName="gn_stroke_width"
						Click="stroke_width_click_async" Text="Others..." />
                </MenuFlyoutSubItem>
                <!-- 端の形式 -->
                <MenuFlyoutSubItem
					x:Name="mfsi_cap_style_2" x:Uid="mfsi_cap_style"
					Text="Cap Style ">
                    <MenuFlyoutSubItem.Icon>
                        <PathIcon Data="{StaticResource data_cap_square}" />
                    </MenuFlyoutSubItem.Icon>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_cap_style_flat_2" x:Uid="rmfi_cap_style_flat" GroupName="gn_cap_style" 
						Click="cap_style_click"
						Text="Flat" IsChecked="true">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_cap_flat}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_cap_style_square_2" x:Uid="rmfi_cap_style_square" GroupName="gn_cap_style" 
						Click="cap_style_click"
						Text="Square" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_cap_square}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_cap_style_round_2" x:Uid="rmfi_cap_style_round" GroupName="gn_cap_style"
						Click="cap_style_click" Text="Round" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_cap_round}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_cap_style_triangle_2" x:Uid="rmfi_cap_style_triangle" GroupName="gn_cap_style"
						Click="cap_style_click" Text="Triangle">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_cap_triangle}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                </MenuFlyoutSubItem>
                <!-- 線の結合の形式 -->
                <MenuFlyoutSubItem
                    x:Name="mfsi_join_style_2" x:Uid="mfsi_join_style" Text="Join Style ">
                    <MenuFlyoutSubItem.Icon>
                        <PathIcon Data="{StaticResource data_join_bevel}" />
                    </MenuFlyoutSubItem.Icon>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_join_style_miter_or_bevel_2" x:Uid="rmfi_join_style_miter_or_bevel" GroupName="gn_join_style"
						Click="join_style_click" Text="Miter" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_join_miter}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_join_style_round_2" x:Uid="rmfi_join_style_round" GroupName="gn_join_style"
						Click="join_style_click" Text="Rounded" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_join_round}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem
						x:Name="rmfi_join_style_bevel_2" x:Uid="rmfi_join_style_bevel" GroupName="gn_join_style"
						Click="join_style_click" Text="Beveled" >
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_join_bevel}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <!--
					<muxc:RadioMenuFlyoutItem
						x:Name="rmfi_join_style_miter" x:Uid="rmfi_join_style_miter" GroupName="gn_join_style"
						Click="join_style_click" IsChecked="true" Text="Shape" >
						<muxc:RadioMenuFlyoutItem.Icon>
							<PathIcon Data="{StaticResource data_join_miter}" />
						</muxc:RadioMenuFlyoutItem.Icon>
					</muxc:RadioMenuFlyoutItem>
					-->
                    <MenuFlyoutSeparator />
                    <MenuFlyoutItem
						x:Name="mfi_join_miter_limit_2" x:Uid="mfi_join_miter_limit"
						Click="join_miter_limit_click_async" Text="Miter Limit..." />
                </MenuFlyoutSubItem>
                <!-- ================ -->
                <MenuFlyoutSeparator x:Name="mfs_separator_stroke_arrow"/>
                <!-- 矢じるしの形式 -->
                <MenuFlyoutSubItem
                    x:Name="mfsi_arrow_style_2"
                    x:Uid="mfsi_arrow_style" Text="Arrowhead Style">
                    <MenuFlyoutSubItem.Icon>
                        <PathIcon Data="{StaticResource data_arrow_opened}" />
                    </MenuFlyoutSubItem.Icon>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_arrow_style_none_2" x:Uid="rmfi_arrow_style_none" Click="arrow_style_click" IsChecked="True" Text="None" GroupName="gn_arrow_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_line}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <MenuFlyoutSeparator />
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_arrow_style_opened_2" x:Uid="rmfi_arrow_style_opened" Click="arrow_style_click" Text="Opened" GroupName="gn_arrow_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_arrow_opened}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_arrow_style_filled_2" x:Uid="rmfi_arrow_style_filled" Click="arrow_style_click" Text="Filled" GroupName="gn_arrow_style">
                        <muxc:RadioMenuFlyoutItem.Icon>
                            <PathIcon Data="{StaticResource data_arrow_filled}" />
                        </muxc:RadioMenuFlyoutItem.Icon>
                    </muxc:RadioMenuFlyoutItem>
                </MenuFlyoutSubItem>
                <!-- 矢じるしの大きさ -->
                <MenuFlyoutItem
                    x:Name="mfi_arrow_size_2" x:Uid="mfi_arrow_size"
                    Click="arrow_size_click_async" IsEnabled="False" Text="Arrowhead Size..." />
                <!-- ================ -->
                <MenuFlyoutSeparator x:Name="mfs_separator_arrow_color"/>
                <!-- 線の色 -->
                <MenuFlyoutItem
                    x:Name="mfi_stroke_color_2" x:Uid="mfi_stroke_color"
                    Click="stroke_color_click" Text="Stroke Color..." >
                    <MenuFlyoutItem.Icon>
                        <PathIcon Data="{StaticResource data_stroke_color}" />
                    </MenuFlyoutItem.Icon>
                </MenuFlyoutItem>
                <!-- 塗りつぶしの色 -->
                <MenuFlyoutItem
                    x:Name="mfi_fill_color_2" x:Uid="mfi_fill_color" Click="fill_color_click" Text="Fill Color..." >
                    <MenuFlyoutItem.Icon>
                        <PathIcon Data="{StaticResource data_fill_color}" />
                    </MenuFlyoutItem.Icon>
                </MenuFlyoutItem>
                <!-- ================ -->
                <!-- 書体名 -->
                <MenuFlyoutItem x:Name="mfi_font_family_2" x:Uid="mfi_font_family" Click="font_family_click_async" Icon="Font" Text="Font Family..." />
                <!-- 書体の大きさ -->
                <MenuFlyoutItem x:Name="mfi_font_size_2" x:Uid="mfi_font_size" Click="font_size_click_async" Icon="FontSize" Text="Font Size..." />
                <!-- 書体の太さ -->
                <MenuFlyoutSubItem x:Name="mfsi_font_weight_2" x:Uid="mfsi_font_weight" Icon="Bold" Text="Font Weight" >
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_font_weight_thin_2" x:Uid="rmfi_font_weight_thin" Click="font_weight_click" Text="Thin (100)" GroupName="gn_font_wieght"/>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_font_weight_extra_light_2" x:Uid="rmfi_font_weight_extra_light" Click="font_weight_click" Text="Extra Light (200)" GroupName="gn_font_wieght"/>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_font_weight_light_2" x:Uid="rmfi_font_weight_light" Click="font_weight_click" Text="Light (300)" GroupName="gn_font_wieght"/>
                    <muxc:RadioMenuFlyoutItem x:Name="rmfi_font_weight_normal_2" x:Uid="rmfi_font_weight_normal" Click="font_weight_click" Text="Normal (400)" GroupName="gn_font_wieght"/>
